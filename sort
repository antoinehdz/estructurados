import re

def sorted_numerically(df):
    def extract_number(col):
        match = re.search(r'\d+$', col)
        return int(match.group()) if match else None
    
    column_order = sorted(df.columns, key=extract_number)
    return df[column_order]

import pandas as pd
import numpy as np

# Simulando un DataFrame
df_proyectos = pd.DataFrame(np.random.randint(0, 100, size=(5, 100)),
                            columns=[f'Proyecto {i+1}' for i in range(100)])

# Aplicar la función de ordenamiento
df_proyectos_sorted = sorted_numerically(df_proyectos)
print(df_proyectos_sorted.columns)  # Debería mostrar las columnas ordenadas numéricamente
