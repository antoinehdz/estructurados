import quandl

def descargar_datos_quandl(ticker):
    """
    Descarga datos históricos para un índice utilizando Quandl.
    
    Args:
    ticker (str): Ticker del índice a descargar.
    
    Returns:
    DataFrame: Un DataFrame con los datos históricos.
    """
    # Configura tu "api_key" personal de Quandl
    quandl.ApiConfig.api_key = 'tu_api_key'
    
    # Obtener datos
    data = quandl.get(ticker, start_date='2023-01-01', end_date='2023-01-31')
    return data

# Ejemplo de uso
ticker = "BSE/BOM503805"  # Debes encontrar el código correcto para IRTCOMP en Quandl
datos_hist = descargar_datos_quandl(ticker)
print(datos_hist)
